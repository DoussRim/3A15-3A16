<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Description</th>
        <th>Details</th>
        <th>Update</th>
    </tr>
    {% for c in cc%}
    <tr>
        <td>{{c.id}}</td>
        <td>{{c.name}}</td>
        <td>{{c.description}}</td>
        <td><a href="{{path('DD',{'id':c.id})}}">Detail</a></td>
        <td><a href="{{path('Del',{'id':c.id})}}">Delete</a></td>
        <td><a href="{{path('Update',{'id':c.id})}}">Update</a></td>
    </tr>
    {%endfor%}
</table>